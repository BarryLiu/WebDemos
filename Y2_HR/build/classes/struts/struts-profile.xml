			<result name="human_check">/page/profile/human_check.jsp</result>
			
			<result name="check_list">/page/profile/check_list.jsp</result>
		    
		    
			<result name="query_locate">/page/profile/query_locate.jsp</result>
			<result name="query_list">/page/profile/query_list.jsp</result>
		<!-- 人力资源档案变更 -->
			<result name="change_locate">/page/profile/change_locate.jsp</result>
			<result name="change_list">/page/profile/change_list.jsp</result>
