	<package name="profile" namespace="/profile" extends="struts-default">
		<action name="profileAction" class="profileAction">
		<!-- 人力资源档案登记 -->
			<result name="human_register">/page/profile/human_register.jsp</result>
			<result name="register_choose_picture">/page/profile/register_choose_picture.jsp</result>
		
